{
  "rules": {
    "member-access": [true, "no-public"],
    "ordered-imports": false,
    "interface-name": false,
    "curly": false,
    "jsx-no-lambda": false
  },
  "extends": ["tslint-react", "tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts", "coverage/lcov-report/*.js"]
  }
}
